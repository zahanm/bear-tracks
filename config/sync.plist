<plist version="1.0">
<dict>
    <key>Label</key>
    <string>edu.zahanm.bear-tracks.sync</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/fdautil</string>
        <string>exec</string>
        <string>/Users/zahanm/source/bear-tracks/bin/index.sh</string>
        <string>sync</string>
        <string>/Users/zahanm/Documents/Bear Notes</string>
        <string>--write</string>
